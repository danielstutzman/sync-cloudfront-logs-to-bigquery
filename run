#!/bin/bash -ex
cd $GOPATH/src/github.com/danielstutzman/sync-cloudfront-logs-to-bigquery
go vet github.com/danielstutzman/sync-cloudfront-logs-to-bigquery/src/...
rm -f $GOPATH/bin/deployer
GOBIN=$GOPATH/bin go install -race github.com/danielstutzman/sync-cloudfront-logs-to-bigquery/src/...
exec $GOPATH/bin/deployer "$@"
